{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": false,
  "sourceRoot": "src",
  "entryFile": "index",
  "language": "ts",
  "generateOptions": {
    "spec": false
  },
  "compilerOptions": {
    "typeCheck": true,
    "builder": {
      "type": "swc",
      "options": {
        "swcrcPath": "./.swcrc"
      }
    }
  }
}
